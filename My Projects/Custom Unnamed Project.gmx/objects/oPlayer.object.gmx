// movement

if firstHit != 0 &amp;&amp; Invincibility != 0
{
    Invincibility = Invincibility - 1
}


if keyboard_check(ord('W'))  //when W is pressed
{
    y = y-playerSpeed
}

if keyboard_check(ord('S'))  //when S is pressed
{
    y = y+playerSpeed
}

if keyboard_check(ord('A'))  //when A is pressed
{
    x = x-playerSpeed
}

if keyboard_check(ord('D'))  //when D is pressed
{
    x = x+playerSpeed
}

// facing

image_angle = point_direction(x,y, mouse_x, mouse_y)

// shooting

if mouse_check_button(mb_left)
{
    
    myBullet = instance_create(x, y, oBullet)    
    myBullet.direction = image_angle
    myBullet.speed = 900 / room_speed
    myBullet.image_angle = image_angle
    
    // bullet kick
    
    kick = 90 / room_speed
    kickDirection = image_angle  + 180
    x = x + lengthdir_x(kick, kickDirection)
    y = y + lengthdir_y(kick, kickDirection)
    audio_play_sound(aNormalBullet1, 1, false)
}

// stay inside room

if (x &lt; 0) { x = 0}
if (x &gt; room_width) {x = room_width}
if (y &lt; 0){y = 0}
if (y &gt; room_height){y = room_height}

// Death

firstHit = playerHitEnemy(oEnemy1, firstHit, Invincibility)
firstHit = playerHitEnemy(oEnemy2, firstHit, Invincibility)
firstHit = playerHitEnemy(oEnemy2DestroyedBits, firstHit, Invincibility)

// below code is equivalent to above its here just so I understand it

x = clamp(x, 0, room_width)
y = clamp(y, 0, room_height)


